{{!
    Page footer.
}}
<!-- klassplace/footer -->
<footer id="page-footer" class="text-light" role="contentinfo">
    <div class="ocean">
        <svg class="waves" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
            viewBox="0 24 150 28" preserveAspectRatio="none" shape-rendering="auto">
            <defs>
            <path id="gentle-wave" d="M-160 44c30 0 58-18 88-18s 58 18 88 18 58-18 88-18 58 18 88 18 v44h-352z" />
            </defs>
            <g class="parallax">
            <use xlink:href="#gentle-wave" x="48" y="0" />
            <use xlink:href="#gentle-wave" x="48" y="3" />
            <use xlink:href="#gentle-wave" x="48" y="5" />
            <use xlink:href="#gentle-wave" x="48" y="7" />
            </g>
        </svg>
    </div>
    <div id="footer-info" class="container">

        {{#hascoursefooter}}
        <div id="course-footer">{{{output.course_footer}}}</div>
        {{/hascoursefooter}}

        {{#hasfooterelements}}
        <div class="container">
            <div class="row">
                <div class="col-md-4 left-footer">
                {{{leftfooter}}}
                </div>
                <div class="col-md-4 middle-footer">
                {{{midfooter}}}
                </div>
                <div class="col-md-4 right-footer">
                {{{rightfooter}}}
                </div>
            </div>
        </div>
        {{/hasfooterelements}}

        {{#hasfootnote}}
        <div class="footnote">{{{footnote}}}</div>
        {{/hasfootnote}}

        {{#hasdoclink}}
        <!-- pagedoc -->
        <div class="doclink">
            <p class="helplink">{{{pagedoclink}}}</p>
        </div>
        <!-- /pagedoc -->
        {{/hasdoclink}}

        {{{ output.login_info }}}

        <nav class="nav navbar-nav d-md-none" role="navigation" aria-label="{{#str}}mainmenu, theme_klassplace{{/str}}">
            {{# output.custom_menu_flat }}
                <ul class="list-unstyled pt-3">
                    {{> theme_boost/custom_menu_footer }}
                </ul>
            {{/ output.custom_menu_flat }}
        </nav>

        <!-- standard footer html -->
        {{{ output.standard_footer_html }}}
        <!-- /standard footer html -->
        <!-- standard end of body html -->
        {{{ output.standard_end_of_body_html }}}
        <!-- /standard end of body html -->

    </div>
    <div class="footer-content-debugging footer-dark bg-dark text-light">
        <div class="container-fluid footer-dark-inner">
            {{{ output.debug_footer_html }}}
        </div>
    </div>
</footer>
<!-- /klassplace/footer -->
