<!-- theme_klassplace/social_links -->
<div id="socialnav-footer" class="row-fluid">
    <ul class="social pull-left">
        {{#social}}
        {{#display}}
        <li>
            <a href="{{{url}}}" target="_blank">
                <i class="fa fa-{{name}}"></i>
            </a>
        </li>
        {{/display}}
        {{/social}}
    </ul>

    <!-- End Top Social Section -->
    {{#displaycontactno}}
    <div class="contact pull-right">
        <i class="fa fa-phone"></i> <a href="tel:{{contactno}}">{{{contactno}}}</a>
    </div>
    {{/displaycontactno}}
</div>
<!-- /theme_klassplace/social_links -->
